{
  "name": "@canlooks/reactive",
  "version": "4.7.10",
  "author": "C.CanLiang <canlooks@gmail.com>",
  "description": "A react tool for responding data and managing state.",
  "keywords": [
    "react",
    "reactive",
    "responsive",
    "automatic",
    "dynamic",
    "state management"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./react": {
      "types": "./react/index.d.ts",
      "import": "./react/index.mjs",
      "require": "./react/index.js"
    },
    "./forage": {
      "types": "./forage/index.d.ts",
      "import": "./forage/index.mjs",
      "require": "./forage/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canlooks/reactive"
  },
  "homepage": "https://github.com/canlooks/reactive",
  "bugs": {
    "url": "https://github.com/canlooks/reactive/issues",
    "email": "canlooks@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "clean": "npx shx rm -rf dist",
    "build": "tsc -m esnext --outDir dist/esm & tsc -m commonjs --outDir dist/cjs",
    "build:resolvePaths": "tsc-alias --outDir dist/esm",
    "rebuild": "npm run clean && npm run build && npm run build:resolvePaths",
    "test:browser": "vite -c test/vite.config.mts",
    "test:unit": "npx ts-node test/unit.ts",
    "test:js": "node test/index.js"
  },
  "dependencies": {
    "globals": "^16.5.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "localforage": "^1.10.0",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
